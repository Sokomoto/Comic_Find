   <div class="row field form-group form-inline">
    <span class="col-xs-3"><strong>プロフィール画像</strong></span>
    <%= f.attachment_field :profile_image %>
  </div>

  <div class="row field form-group form-inline">
    <span class="col-xs-3"><strong>ユーザー名</strong></span>
    <%= f.text_field :name, autofocus: true, autocomplete: "name",class:"form-control col-xs-2" %>
  </div>

  <div class="row field form-group form-inline">
    <span class="col-xs-3"><strong>メールアドレス</strong></span>
    <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"form-control" %>
  </div>
